{
  "name": "wdb/dependencies",
  "description": "TYPO3 backend module for analyzing dependencies, including versions, with categories and tags related to Composer, npm and Solr.",
  "type": "typo3-cms-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "David Bruchmann",
      "email": "david.bruchman@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "typo3/cms-core": "^12.4 || ^13.0 || ^14.0"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "dependencies"
    }
  },
  "autoload": {
    "psr-4": {
      "Wdb\\Dependencies\\": "Classes/"
    }
  }
}
